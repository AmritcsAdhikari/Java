<!DOCTYPE web-app
      PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
      "http://java.sun.com/dtd/web-app_2_3.dtd">


<web-app>
	<servlet>
		<servlet-name>ServletOne</servlet-name>
		<display-name>ServletOne</display-name>
		<description>this is one</description>
		<servlet-class>com.test.httpSessionDemo.com.test.httpSessionDemo.ServletOne</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ServletTwo</servlet-name>
		<display-name>ServletTwo</display-name>
		<description>this is two</description>
		<servlet-class>com.test.httpSessionDemo.com.test.httpSessionDemo.ServletTwo</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ServletOne</servlet-name>
		<url-pattern>/ServletOne</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServletTwo</servlet-name>
		<url-pattern>/ServletTwo</url-pattern>
	</servlet-mapping>
</web-app>